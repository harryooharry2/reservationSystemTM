# Production Environment Variables for Frontend
# Copy this file to .env.production and fill in your actual values

# Supabase Configuration
PUBLIC_SUPABASE_URL=https://your-production-project.supabase.co
PUBLIC_SUPABASE_ANON_KEY=your-production-anon-key

# API Configuration
PUBLIC_API_URL=https://your-backend-api.onrender.com
PUBLIC_API_BASE=https://your-backend-api.onrender.com/api

# Application Configuration
PUBLIC_APP_NAME=Cafe Reservation System
PUBLIC_APP_VERSION=1.0.0
PUBLIC_APP_ENVIRONMENT=production

# Feature Flags
PUBLIC_ENABLE_ANALYTICS=true
PUBLIC_ENABLE_DEBUG=false 